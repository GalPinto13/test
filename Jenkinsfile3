pipeline {
    agent none

    stages {
        stage('Run Tests') {
            parallel {
                stage('Agent 1') {
                    agent {
                        label 'master'
                    }
                    steps {
                        
                        println "Linux Operating System Version: ${System.getProperty('os.version')}"
                    }
                }

                stage('Agent 2') {
                    agent {
                        label 'windows'
                    }
                    steps {
                       
                        println "Windows Operating System Version: ${System.getProperty('os.version')}"
                    }
                }
            }
        }
    }
}
